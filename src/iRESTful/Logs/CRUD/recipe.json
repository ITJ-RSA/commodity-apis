{
    "name": "iRESTful/LogCrudApi",
    "version": "17.02.21",
    "license": "MIT",
    "type": "crud-rest-api",
    "urls": {
        "repository": "https://github.com/irestful-labs/log-crud-api",
        "homepage": "https://github.com/irestful-labs/log-crud-api"
    },
    "authors": [
        {
            "name": "Steve Rodrigue",
            "email": "steve@irestful.net",
            "url": "https://irestful.net"
        }
    ],
    "maintainer": "Steve Rodrigue",
    "project": {
        "samples": "samples.json",
        "code": {
            "language": "PHP",
            "file": "code.php"
        },
        "objects": {
            "message": {
                "database": "pdo",
                "properties": {
                    "type": "type",
                    "message": "string",
                    "ip_address|?": "ip_address",
                    "domain|?": "domain",
                    "username|?": "username"
                }
            },
            "type": {
                "database": "pdo",
                "properties": {
                    "keyname": "keyname",
                    "title": "string",
                    "description": "string"
                }
            }
        },
        "databases": {
            "pdo": "relational"
        },
        "converters": [
            {
                "from": "string",
                "to": "domain"
            },
            {
                "from": "string",
                "to": "ip_address"
            },
            {
                "from": "string",
                "to": "username"
            },
            {
                "from": "string",
                "to": "keyname"
            }
        ],
        "types": {
            "domain": {
                "database_type": {
                    "name": "string",
                    "max": 255
                },
                "function": "validateDomain",
                "converters": {
                    "database_to_object": {
                        "from": "string",
                        "to": "domain"
                    }
                }
            },
            "ip_address": {
                "database_type": {
                    "name": "string",
                    "max": 255
                },
                "function": "validateIpAddress",
                "converters": {
                    "database_to_object": {
                        "from": "string",
                        "to": "ip_address"
                    }
                }
            },
            "username": {
                "database_type": {
                    "name": "string",
                    "max": 255
                },
                "function": "validateUsername",
                "converters": {
                    "database_to_object": {
                        "from": "string",
                        "to": "username"
                    }
                }
            },
            "keyname": {
                "database_type": {
                    "name": "string",
                    "max": 255
                },
                "function": "validateKeyname",
                "converters": {
                    "database_to_object": {
                        "from": "string",
                        "to": "keyname"
                    }
                }
            }
        }
    }
}
